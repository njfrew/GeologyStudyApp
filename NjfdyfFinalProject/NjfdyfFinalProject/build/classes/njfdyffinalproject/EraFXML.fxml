<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="njfdyffinalproject.EraController">
   <children>
      <MenuBar>
        <menus>
          <Menu fx:id="erasMenu" mnemonicParsing="false" text="Eons" />
          <Menu mnemonicParsing="false" text="Quiz">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#handleOpen" text="Take Quiz" />
                  <MenuItem mnemonicParsing="false" onAction="#switchToGrade" text="Quiz Grades" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" text="About">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#switchToAbout" text="Open" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" styleClass="mainFxmlClass">
         <children>
            <AnchorPane fx:id="vizPane" layoutX="16.0" layoutY="57.0" prefHeight="469.0" prefWidth="753.0">
               <children>
                  <ImageView fx:id="eraImageView" fitHeight="469.0" fitWidth="380.0" pickOnBounds="true" preserveRatio="true" />
                  <TextArea fx:id="contentArea" editable="false" layoutX="380.0" prefHeight="469.0" prefWidth="372.0" wrapText="true" />
               </children></AnchorPane>
            <Text fx:id="eraNameText" fill="RED" layoutX="25.0" layoutY="592.0" strokeType="OUTSIDE" strokeWidth="0.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
